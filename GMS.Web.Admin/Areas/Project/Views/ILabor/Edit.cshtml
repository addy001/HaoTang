@using GMS.Framework.Contract
@using GMS.Framework.Utility
@using GMS.Framework.Web.Controls
@using GMS.Project.Contract

@model InsLaborCost
@{
    Layout = "~/Views/Shared/_Layout.Edit.cshtml";
}
@section PageSpecificStyleSheetIncludes{
}
@section PageSpecificJavascriptIncludes{
    <script type="text/javascript">
    </script>
}
@section MainContent{
    <div class="portlet-body form-horizontal form-bordered form-row-stripped">
        <div class="row-fluid">
            <div class="control-group">
                <label class="control-label"><span class="required">*</span>项目名称：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.ProjectName, new { @class = "m-wrap" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.ProjectName)</span>
                </div>
            </div>

            <div class="control-group">
                <label class="control-label">泥瓦工费用：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.Masons, new { @class = "m-wrap small" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.Masons)</span>
                </div>
            </div>

            <div class="control-group">
                <label class="control-label">拆除人工费用：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.TearDown, new { @class = "m-wrap small" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.TearDown)</span>
                </div>
            </div>
            <div class="control-group">
                <label class="control-label">木工费用：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.Carpenter, new { @class = "m-wrap large" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.Carpenter)</span>
                </div>
            </div>
            <div class="control-group">
                <label class="control-label">水电工费用：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.Plumbers, new { @class = "m-wrap large" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.Plumbers)</span>
                </div>
            </div>
           
            <div class="control-group">
                <label class="control-label">油漆工费用：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.Painter, new { @class = "m-wrap large" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.Painter)</span>
                </div>
            </div>
            <div class="control-group">
                <label class="control-label">电焊工费用：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.ELectricWelder, new { @class = "m-wrap large" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.ELectricWelder)</span>
                </div>
            </div>
            <div class="control-group">
                <label class="control-label">钢筋工费用：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.Steel, new { @class = "m-wrap large" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.Steel)</span>
                </div>
            </div>
            <div class="control-group">
                <label class="control-label">搬运工费用：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.Transportor, new { @class = "m-wrap large" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.Transportor)</span>
                </div>
            </div>
            <div class="control-group">
                <label class="control-label">垃圾清理工费：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.Cleaner, new { @class = "m-wrap large" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.Cleaner)</span>
                </div>
            </div>
            <div class="control-group">
                <label class="control-label">其他费用：</label>
                <div class="controls">
                    @Html.TextBoxFor(m => m.OtherLabor, new { @class = "m-wrap large" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.OtherLabor)</span>
                </div>
            </div>
           
        </div>
    </div>
}

